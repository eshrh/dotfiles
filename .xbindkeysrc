"pactl set-sink-volume @DEFAULT_SINK@ +10%"
    m:0x40 + c:54
    Mod4 + c
"pactl set-sink-volume @DEFAULT_SINK@ -10%"
    m:0x40 + c:53
    Mod4 + x
"pactl set-sink-mute @DEFAULT_SINK@ toggle"
    m:0x40 + c:52
    Mod4 + z

"mpc next || cmus-remote -n"
    m:0x8 + c:58
    Alt_L + m
"mpc prev || cmus-remote -r"
    m:0x8 + c:57
    Alt_L + n
"mpc toggle || cmus-remote -u"
    m:0x8 + c:56
    Alt_L + b

"rofi -show run"
    m:0x8 + c:65
    Alt_L + space
"rofi -show window"
    m:0x1 + m:0x8 + c:65
    Shift_L + Alt_L + space

